{
    v_import("printf");
}

{
    char_ptr = LLVMPointerType(char, 0);

    arg = v_alloca(LLVMTypeRef);

    v_store(char_ptr, arg);

    puts_ft = LLVMFunctionType(int, arg, 1, 0);

    v_add_symbol_type("puts", puts_ft);
}

{
    puts("Hello, world!");
}

{
    puts("Hello again!");

    printf("Hi %d times!\n", 10000);
}

{
    puts("Превед!");
}


//---------------------------------------------------------------------
//- mk_stmt_grammar - grammar action
//---------------------------------------------------------------------
{
    //-----------------------------------------------------------------
    f = v_function_hack("mk_stmt_grammar_grammar_action", v_peg_grammar_action_fun_t);

    v_add_parameter_name(f, 0, "ret",       v_util_std_any_ref);
    v_add_parameter_name(f, 1, "any0",      v_util_std_any_ref);
    v_add_parameter_name(f, 2, "any_count", size_t);
}
{
    gr_stmt_list = v_alloca(v_ast_opaque_stmt_list_ptr);
    v_initialize(gr_stmt_list);

    v_ast_make_stmt_list_nil(gr_stmt_list);


    //-----------------------------------------------------------------
    //- Parsers "stack"

    pr_count = v_load(voidc_ast_stmt_grammar_parsers_count);


    voidc_ast_stmt_grammar_stack_init(gr_stmt_list,
                                      "v_peg_opaque_parser_ptr",
                                      "v.pr",
                                      pr_count,
                                      1
                                     );


    //-----------------------------------------------------------------
    //- Arguments "stack"

    ar_count = v_load(voidc_ast_stmt_grammar_arguments_count);


    voidc_ast_stmt_grammar_stack_init(gr_stmt_list,
                                      "v_peg_opaque_argument_ptr",
                                      "v.ar",
                                      ar_count,
                                      1
                                     );


    //-----------------------------------------------------------------
    //- Ranges "stack"

    rn_count = v_load(voidc_ast_stmt_grammar_ranges_count);


    voidc_ast_stmt_grammar_stack_init(gr_stmt_list,
                                      "v_peg_class_parser_range_t",
                                      "v.rn",
                                      rn_count,
                                      0
                                     );


    //-----------------------------------------------------------------
    //- ...

    arg0 = v_alloca(v_ast_opaque_argument_ptr, 2);
    v_initialize(arg0, 2);

    arg1 = v_getelementptr(arg0, 1);

    stmt = v_alloca(v_ast_opaque_stmt_ptr);
    v_initialize(stmt);


    //-----------------------------------------------------------------
    //- ...

    v_ast_make_arg_identifier(arg0, "v_peg_opaque_action_ptr");

    v_ast_make_stmt_call(stmt, "v.act", "v_alloca", arg0, 1);

    v_ast_make_stmt_list(gr_stmt_list, gr_stmt_list, stmt);


    v_ast_make_arg_identifier(arg0, "v.act");

    v_ast_make_stmt_call(stmt, "", "v_initialize", arg0, 1);

    v_ast_make_stmt_list(gr_stmt_list, gr_stmt_list, stmt);


    //-----------------------------------------------------------------
    //- ...

    v_ast_make_arg_identifier(arg0, "int");

    v_ast_make_stmt_call(stmt, "v.int", "v_alloca", arg0, 1);

    v_ast_make_stmt_list(gr_stmt_list, gr_stmt_list, stmt);


    //-----------------------------------------------------------------
    //- ...

    v_ast_make_stmt_block(stmt, voidc_ast_stmt_grammar_stmt_list, 0);

    v_ast_make_stmt_list(gr_stmt_list, gr_stmt_list, stmt);


    //-----------------------------------------------------------------
    //- ...

    v_ast_make_arg_identifier(arg0, "v.act");

    v_ast_make_stmt_call(stmt, "", "v_terminate", arg0, 1);

    v_ast_make_stmt_list(gr_stmt_list, gr_stmt_list, stmt);


    //-----------------------------------------------------------------
    //- ...

    pr_num = v_cast(pr_count, intptr_t);
    ar_num = v_cast(ar_count, intptr_t);

    //-----------------------------------------------------------------
    //- Arguments "stack" clear

    v_ast_make_arg_identifier(arg0, "v.ar0");
    v_ast_make_arg_integer(arg1, ar_num);

    v_ast_make_stmt_call(stmt, "", "v_terminate", arg0, 2);

    v_ast_make_stmt_list(gr_stmt_list, gr_stmt_list, stmt);

    //-----------------------------------------------------------------
    //- Parsers "stack" clear

    v_ast_make_arg_identifier(arg0, "v.pr0");
    v_ast_make_arg_integer(arg1, pr_num);

    v_ast_make_stmt_call(stmt, "", "v_terminate", arg0, 2);

    v_ast_make_stmt_list(gr_stmt_list, gr_stmt_list, stmt);


    //-----------------------------------------------------------------
    //- ...

    v_ast_make_stmt_block(stmt, gr_stmt_list, 0);

    v_std_any_set_pointer(ret, stmt);


    //-----------------------------------------------------------------
    //- ...

    v_terminate(stmt);
    v_terminate(arg0, 2);
    v_terminate(gr_stmt_list);

    v_terminate(voidc_ast_stmt_grammar_stmt_list);
    v_terminate(voidc_ast_stmt_grammar_grammar_arg);
}


//---------------------------------------------------------------------
//- mk_gr_stmt_parser - grammar action
//---------------------------------------------------------------------
{
    //-----------------------------------------------------------------
    f = v_function_hack("mk_gr_stmt_parser_grammar_action", v_peg_grammar_action_fun_t);

    v_add_parameter_name(f, 0, "ret",       v_util_std_any_ref);
    v_add_parameter_name(f, 1, "any0",      v_util_std_any_ref);
    v_add_parameter_name(f, 2, "any_count", size_t);
}
{
    any1 = v_getelementptr(any0, 1);

    pr_sstr = v_std_any_get_pointer(v_util_opaque_std_string, any0);

    pr_str = v_std_string_get(pr_sstr);

    as_sstr = v_std_any_get_pointer(v_util_opaque_std_string, any1);

    as_0 = v_std_string_get(as_sstr);
    as_1 = v_getelementptr(as_0, 1);


    arg0 = v_alloca(v_ast_opaque_argument_ptr, 5);
    v_initialize(arg0, 5);

    arg1 = v_getelementptr(arg0, 1);
    arg2 = v_getelementptr(arg0, 2);
    arg3 = v_getelementptr(arg0, 3);
    arg4 = v_getelementptr(arg0, 4);

    stmt = v_alloca(v_ast_opaque_stmt_ptr);
    v_initialize(stmt);

    stmt_list = v_alloca(v_ast_opaque_stmt_list_ptr);
    v_initialize(stmt_list);

    v_copy(stmt_list, voidc_ast_stmt_grammar_stmt_list);


    c1 = v_load(as_1);

    cz = v_cast(0, char);

    is_z = v_icmp(LLVMIntEQ, c1, cz);

    if (is_z)       //- Straight assignment: "="
    {
        v_copy(arg0, voidc_ast_stmt_grammar_grammar_arg);
        v_copy(arg1, voidc_ast_stmt_grammar_grammar_arg);

        v_ast_make_arg_string(arg2, pr_str);


        pr_num = v_load(voidc_ast_stmt_grammar_parser_number);

        if (pr_num)     //- Assign parser from v.pr0  (BTW - assert(pr_num == 1);)
        {
            v_ast_make_arg_identifier(arg3, "v.pr0");       //- Sic!

            v_ast_make_arg_integer(arg4, 0);

            v_ast_make_stmt_call(stmt, "", "v_peg_grammar_set_parser", arg0, 5);
        }
        else            //- Erase parser
        {
            v_ast_make_stmt_call(stmt, "", "v_peg_grammar_erase_parser", arg0, 3);
        }

        v_ast_make_stmt_list(stmt_list, stmt_list, stmt);
    }
    else
    {
        v_copy(arg0, voidc_ast_stmt_grammar_grammar_arg);

        v_ast_make_arg_string(arg1, pr_str);

        v_ast_make_arg_identifier(arg2, "v.pr1");

        v_ast_make_arg_identifier(arg3, "v.int");       //- "leftrec" !

        v_ast_make_stmt_call(stmt, "", "v_peg_grammar_get_parser", arg0, 4);

        v_ast_make_stmt_list(stmt_list, stmt_list, stmt);


        v_ast_make_arg_identifier(arg0, "v.pr0");       //- Target


        c0 = v_load(as_0);

        ce = v_cast('=', char);

        is_e = v_icmp(LLVMIntEQ, c0, ce);

        c_k = v_alloca(char);       //- To obtain '*' or '+'

        if (is_e)       //- Append to back
        {
            v_store(c1, c_k);

            v_ast_make_arg_identifier(arg1, "v.pr1");       //- From grammar
            v_ast_make_arg_identifier(arg2, "v.pr0");       //- Built
        }
        else            //- Append to front
        {
            v_store(c0, c_k);

            v_ast_make_arg_identifier(arg1, "v.pr0");       //- Built
            v_ast_make_arg_identifier(arg2, "v.pr1");       //- From grammar
        }


        k = v_load(c_k);

        cp = v_cast('+', char);

        is_p = v_icmp(LLVMIntEQ, k, cp);

        if (is_p) v_ast_make_stmt_call(stmt, "", "v_peg_choice_parser_concat",   arg0, 3);
        else      v_ast_make_stmt_call(stmt, "", "v_peg_sequence_parser_concat", arg0, 3);

        v_ast_make_stmt_list(stmt_list, stmt_list, stmt);


        v_ast_make_arg_identifier(arg0, "v.int");

        v_ast_make_stmt_call(stmt, "leftrec", "v_load", arg0, 1);

        v_ast_make_stmt_list(stmt_list, stmt_list, stmt);


        v_copy(arg0, voidc_ast_stmt_grammar_grammar_arg);
        v_copy(arg1, voidc_ast_stmt_grammar_grammar_arg);

        v_ast_make_arg_string(arg2, pr_str);

        v_ast_make_arg_identifier(arg3, "v.pr0");       //- Sic!

        v_ast_make_arg_identifier(arg4, "leftrec");     //- "leftrec" !

        v_ast_make_stmt_call(stmt, "", "v_peg_grammar_set_parser", arg0, 5);

        v_ast_make_stmt_list(stmt_list, stmt_list, stmt);
    }


    v_copy(voidc_ast_stmt_grammar_stmt_list, stmt_list);

    v_store(0, voidc_ast_stmt_grammar_parser_number);

    v_std_any_set_value(ret, 1);


    v_terminate(stmt_list);
    v_terminate(stmt);
    v_terminate(arg0, 5);
}



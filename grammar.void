//---------------------------------------------------------------------
//- mk_gr_stmt_action - grammar action
//---------------------------------------------------------------------
{
    //-----------------------------------------------------------------
    f = v_function_hack("mk_gr_stmt_action_grammar_action", v_peg_grammar_action_fun_t);

    v_add_parameter_name(f, 0, "ret",       v_util_std_any_ref);
    v_add_parameter_name(f, 1, "any0",      v_util_std_any_ref);
    v_add_parameter_name(f, 2, "any_count", size_t);
}
{

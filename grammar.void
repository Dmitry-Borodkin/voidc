//---------------------------------------------------------------------
{
    typ0 = v_alloca(v_type_ref, 5);
    typ1 = v_getelementptr(typ0, 1);
    typ2 = v_getelementptr(typ0, 2);
    typ3 = v_getelementptr(typ0, 3);
    typ4 = v_getelementptr(typ0, 4);

    v_store(v_ast_stmt_list_ref, typ0);
    v_store(char_ptr,            typ1);
    v_store(char_ptr,            typ2);
    v_store(int,                 typ3);
    v_store(bool,                typ4);

    ft = v_function_type(void, typ0, 5, false);

    //-----------------------------------------------------------------
    f = v_function_hack("voidc_ast_stmt_grammar_stack_init", ft);

    v_add_parameter_name(f, 0, "stmt_list", v_ast_stmt_list_ref);
    v_add_parameter_name(f, 1, "type",      char_ptr);
    v_add_parameter_name(f, 2, "prefix",    char_ptr);
    v_add_parameter_name(f, 3, "size",      int);
    v_add_parameter_name(f, 4, "do_init",   bool);
}
{
    arg0 = v_alloca(v_ast_opaque_argument_ptr, 2);
    v_initialize(arg0, 2);

    arg1 = v_getelementptr(arg0, 1);

    sstr = v_alloca(v_util_opaque_std_string);
    v_initialize(sstr);

    v_ast_make_arg_identifier(arg0, type);

    num = v_cast(size, intptr_t);

    v_ast_make_arg_integer(arg1, num);

    stmt = v_alloca(v_ast_opaque_stmt_ptr);
    v_initialize(stmt);


    v_std_string_set(sstr, prefix);

    v_std_string_append_number(sstr, 0);

    str = v_std_string_get(sstr);


    v_ast_make_stmt_call(stmt, str, "v_alloca", arg0, 2);

    v_ast_make_stmt_list(stmt_list, stmt_list, stmt);


    v_ast_make_arg_identifier(arg0, str);

    if (do_init)
    {
        v_ast_make_stmt_call(stmt, "", "v_initialize", arg0, 2);

        v_ast_make_stmt_list(stmt_list, stmt_list, stmt);
    }


    n_v = v_alloca(intptr_t);

    v_store(1, n_v);

    n1 = v_cast(1, intptr_t);

    loop
    {
        n = v_load(n_v);

        f = v_icmp(LLVMIntUGE, n, num);

        if (f)  v_break();


        v_ast_make_arg_integer(arg1, n);


        v_std_string_set(sstr, prefix);

        v_std_string_append_number(sstr, n);

        str = v_std_string_get(sstr);

        v_ast_make_stmt_call(stmt, str, "v_getelementptr", arg0, 2);

        v_ast_make_stmt_list(stmt_list, stmt_list, stmt);


        n = v_binop(LLVMAdd, n, n1);

        v_store(n, n_v);
    }


    v_terminate(stmt);
    v_terminate(sstr);
    v_terminate(arg0, 2);
}



    v_store(mod_ptr_v, val1);

    llvm_parse_bitcode_2_f = v_obtain_function("LLVMParseBitcode2");

    ok_v = LLVMBuildCall(builder, llvm_parse_bitcode_2_f, val0, 2, "ok_v");



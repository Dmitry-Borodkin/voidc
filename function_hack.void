    LLVMPositionBuilderAtEnd(builder, entry);


    mod_buf_n = LLVMBuildGlobalStringPtr(builder, "mod_buf", "mod_buf_n");

    v_store(buffer,    val0);
    v_store(length,    val1);
    v_store(mod_buf_n, val2);
